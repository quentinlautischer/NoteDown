{
  "name": "NoteDown",
  "description": "Note Taking App",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "undefined"
  },
  "main": "none.js",
  "scripts": {
    "massinstall": "npm install & cd server && npm install && cd .. & cd server/db && npm install && cd ../.. & cd desktop && npm install && cd .. & cd mobile && npm install && cd .. & cd shared && npm install && cd ..",
    "desktop": "cd desktop && electron . && cd ..",
    "desktop_watch_webkit": "cd desktop && npm run dev && cd ..",
    "server": "cd server && node server.js && cd ..",
    "db": "cd server/db && mongod --dbpath ./data/db && cd ../..",
    "ios": "cd mobile && react-native run-ios && cd ..",
    "android": "cd mobile && react-native run-android && cd ..",
    "watch_sass": "cd shared && sass --watch stylesheets && cd ..",
    "mobile_watch_css": "cd mobile && node_modules/.bin/react-native-css -i ../shared/stylesheets/utils/main.css -o styles.js --watch --pretty && cd .."
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "mongoose": "^4.8.2"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.18.0",
    "fs": "0.0.1-security",
    "shelljs": "^0.0.9"
  },
  "license": "MIT"
}
